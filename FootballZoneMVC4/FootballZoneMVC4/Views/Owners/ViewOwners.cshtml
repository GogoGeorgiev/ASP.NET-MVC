@using FootballZoneMVC4.Models;
@model List<OwnersViewModel>

@{
    ViewBag.Title = "Собственици";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 align="center">Футболни собственици</h2>


@foreach (var item in Model)
{
    <div class="cont">
        <div class="image">@Html.Image(item.ImageUrl, item.OwnerNames) </div>
        <div class="name"><p class="nam">@item.OwnerNames </p></div>
        <text class="info">@item.Biography </text>
     </div>   
        
   
}